{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000,immutable"
        },
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000,immutable"
        },
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url",
      "SECRET_KEY": "@secret_key"
    },
    "build": {
      "env": {
        "DATABASE_URL": "@database_url",
        "SECRET_KEY": "@secret_key"
      }
    }
  }
  