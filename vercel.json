{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/wsgi.py"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url",
      "SECRET_KEY": "@secret_key"
    },
    "build": {
      "env": {
        "DATABASE_URL": "@database_url",
        "SECRET_KEY": "@secret_key"
      }
    }
  }
  